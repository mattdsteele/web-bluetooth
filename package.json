{
  "name": "nejs-web-bluetooth",
  "version": "1.0.0",
  "description": "",
  "main": "slides.js",
  "scripts": {
    "clean": "rm -rf web-bluetooth",
    "start": "webpack --watch",
    "cp": "cp -R public/* web-bluetooth && cp node_modules/sw-toolbox/sw-toolbox.js web-bluetooth",
    "build": "webpack",
    "sw": "sw-precache --config=make-sw.js && mv service-worker.js web-bluetooth",
    "dist": "npm run clean && npm run build && npm run cp && npm run sw"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "keyboardjs": "^2.3.3",
    "phaser-ce": "^2.8.3",
    "string.prototype.padstart": "^3.0.0",
    "xstream": "^10.9.0"
  },
  "devDependencies": {
    "eddystone-beacon": "^1.0.7",
    "sw-precache": "^5.2.0",
    "sw-toolbox": "^3.6.0",
    "webpack": "^3.5.4"
  }
}
